{
    "version": 2,
    "builds": [
        {
            "src": "public/index.html",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        }
    ]
}